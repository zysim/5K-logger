        "axios": "^0.18.1",
        "babel-preset-react": "^6.23.0",
        "bootstrap": "^4.3.1",
        "cross-env": "^5.1",
        "jquery": "^3.4.1",
        "laravel-mix": "^4.1.2",
        "lodash": "^4.17.15",
        "popper.js": "^1.12",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "tslint": "^5.19.0",
        "typescript": "^3.5.3"
    }
}
